{
  "name": "skeleton-elements",
  "version": "0.0.1",
  "description": "Skeleton elements",
  "main": "index.js",
  "scripts": {
    "build-font": "python ./build/generate.py && ttf2woff ./src/font/skeleton.ttf ./src/font/skeleton.woff",
    "react": "cross-env NODE_ENV=development webpack-dev-server --config ./build/webpack-react.config.js",
    "vue": "cross-env NODE_ENV=development webpack-dev-server --config ./build/webpack-vue.config.js",
    "svelte": "cross-env NODE_ENV=development webpack-dev-server --config ./build/webpack-svelte.config.js",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx,.svelte .",
    "prettier": "prettier --ignore-path .gitignore --write \"**/*.+(js|json)\"",
    "format": "npm run prettier -- --write",
    "check-format": "npm run prettier -- --list-different",
    "validate": "npm-run-all --parallel check-format lint build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nolimits4web/skeleton-elements.git"
  },
  "keywords": [
    "skeleton",
    "ghost"
  ],
  "author": "Vladimir Kharlampidi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nolimits4web/skeleton-elements/issues"
  },
  "homepage": "https://github.com/nolimits4web/skeleton-elements#readme",
  "devDependencies": {
    "@babel/core": "^7.8.6",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.6",
    "@babel/preset-react": "^7.8.3",
    "@babel/runtime": "^7.8.4",
    "autoprefixer": "^9.7.4",
    "babel-loader": "^8.0.6",
    "clean-css": "^4.2.3",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^7.0.0",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "eslint-plugin-svelte3": "^2.7.3",
    "file-loader": "^5.1.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.1.7",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "normalize.css": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.27",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.0.5",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "rimraf": "^3.0.2",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "svelte": "^3.19.1",
    "svelte-loader": "^2.13.6",
    "ttf2woff": "^2.0.1",
    "vue": "^2.6.11",
    "vue-loader": "^15.9.0",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "dependencies": {}
}
